                              rdfs:range xsd:float ;
                              rdfs:comment "Amount of payment duties that originated in the year."@en ,
                                           "Importe de las obligaciones de pago que han tenido su origen en el ejercicio."@es ;
                              rdfs:isDefinedBy "http://vocab.ciudadesabiertas.es/def/hacienda/presupuesto" ;
                              rdfs:label "net recognized duties"@en ,
                                         "obligaciones reconocidas netas"@es .


###  http://vocab.ciudadesabiertas.es/def/hacienda/presupuesto#pagos
:pagos rdf:type owl:DatatypeProperty ;
       rdfs:domain :EjecucionGasto ;
       rdfs:range xsd:float ;
       rdfs:comment "Amount of the net recognized duties that have been paid."@en ,
                    "Importe de las obligaciones reconocidas netas que se han pagado."@es ;
       rdfs:isDefinedBy "http://vocab.ciudadesabiertas.es/def/hacienda/presupuesto" ;
       rdfs:label "pagos"@es ,
                  "payments"@en .


###  http://vocab.ciudadesabiertas.es/def/hacienda/presupuesto#previsionDefinitiva
:previsionDefinitiva rdf:type owl:DatatypeProperty ;
                     rdfs:domain :EjecucionIngreso ;
                     rdfs:range xsd:float ;
                     rdfs:comment "Es la suma de la previsión inicial y sus modificaciones"@es ,
                                  "It is the sum of the initial estimate and its modifications"@en ;
                     rdfs:isDefinedBy "http://vocab.ciudadesabiertas.es/def/hacienda/presupuesto" ;
                     rdfs:label "final estimate"@en ,
                                "previsión definitiva"@es .


###  http://vocab.ciudadesabiertas.es/def/hacienda/presupuesto#previsionInicial
:previsionInicial rdf:type owl:DatatypeProperty ;
                  rdfs:domain :PresupuestoIngreso ;
